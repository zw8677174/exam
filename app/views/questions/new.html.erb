<%= form_for([@paper, @paper.questions.build]) do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :tag %><br>
      <%= f.text_field :tag %>
    </p>
    <p>
      <%= f.label :body %><br>
      <%= f.text_area :body %>
    </p>
    <p>
      <%= f.label :answer %><br>
      <%= f.text_area :answer %>
    </p>
    <p>
      <%= f.submit %>
    </p>
<% end %>
